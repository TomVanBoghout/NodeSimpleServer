{
  "name": "SimpleNodeServer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm install forever -g && npm install -g bower",
    "build": "tsc -p ./",
    "prestart": "tsc -p ./",
    "start": "forever --minUptime 10000 start src/app.js",
    "stop": "forever stopall"
  },
  "dependencies": {
    "@types/mime": "0.0.29",
    "@types/node": "0.0.2",
    "mime": "1.3.4",
    "typescript": "2.0.8"
  }
}
